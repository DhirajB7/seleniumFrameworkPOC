<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SIGNIN">

	<test name="LOG IN WITH USERNAME AND PASSWORD">

		<classes>

			<class name="pageObjects.RegisterPage">
				<methods>
					<include name="registerPageClickOnSignInLink" />
				</methods>
			</class>

			<class name="pageObjects.SignOnPage">
				<methods>
					<include name="signOnPageSignOn" />
				</methods>
			</class>

			<class name="pageObjects.ReservationPage">
				<methods>
					<include name="reservationPageVerifySignIn" />
				</methods>
			</class>
			

		</classes>
		
	</test><!--Test -->
	
</suite>